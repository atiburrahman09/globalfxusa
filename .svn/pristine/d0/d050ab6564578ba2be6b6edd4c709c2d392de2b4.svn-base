

body{
   background-color: #e5e9ec;  
}

.user-info-wrapper {
    margin: 0px;
    padding:1px;
    
}
.page-content{
    min-height:500px;
}
.page-sidebar.mini {    
    min-height: 480px;
}

.semi-bold {   
    color: purple;
}

.dropdown-menu li a{
    padding:5px !important;
}
span.badge.badge-important.animated.bounceIn {
    background: #E80A0A;
    color:#fff;
}

span.badge.badge-important.pull-right{
     background: #E80A0A;
     color:#fff;
}
span.badge.badge-disable.pull-right{
   background: #E80A0A;
   color:#fff;
}

span.badge.badge-important{
   background: #E80A0A;
   color:#fff; 
}

ul#main-menu-toggle-wrapper{
    margin-left: 5px;
}

.logo {
    margin-right: 15px;
}
.page-sidebar {
    background-color: purple !important;
}
.user-info-wrapper .user-info {
    color: #fff;
    background-color: purple;
    display: inline-block;
}
.header-seperation {
    background-color: #aaa;
}

.header .notifcation-center {
    background-color: #fff;
    border-radius: 4px;
}

.iconset.top-search{
    background: url('icon/views.png') no-repeat;
    opacity: .3;
}
.iconset:hover{
    opacity: 1;
}

.iconset {
    background: url('icon/top-tray.png') no-repeat;
    border-radius: 4px;
    position: relative;
    top: 1px;
    background-position: inherit; 
    width: 25px;
    height: 25px;   
}

.header .nav > li.dropdown .active {
    background-color: #fff;
    border-radius: 4px;
}

.page-sidebar > ul > li > a {
    color: #fff;
  
}

.page-sidebar > ul > li a i {
    color: #fff;
}

.menu-title {
    color: #999;
    font-weight: bold;
}

.status-widget p {
    color: #aaa;
}

.footer-widget {   
    background-color: #eee;
}

.footer-widget .details-status {
    background-color: purple;
    color: #fff;    
}
.footer-widget a:hover, .footer-widget a:hover i {
    color: purple;  
}
@media (max-width: 979px){    
    .navbar .nav {
    display:none;
    }
    /*
    ul.nav{
        width:100%;
    }
   .navbar .nav > li{
        float:none;
    } 
    */
}

.chat-window-wrapper input[type="text"]{
    background-color: #fff;
}

.chat-window-wrapper input[type="text"]:active{
    background-color: #999;
}

.popover-content {
    width: 150px;
}

.popname{
    color:blue;
}

.page-title i{
    color: green;
}
.page-title i:hover{
    color: purple;
}

.label-important, .badge-important {
    background-color: purple;
}

select {
    width: 220px;
}

.home-header-top{
    padding: 15px;
}
.home-header-top ul{
    list-style: none;   
}

.home-header-top ul li{
    display: inline-block;
}
.social img{
    opacity: .3;
}
.social img:hover{
   opacity: 1; 
}


.navbar .nav>li a{   
    padding: 20px 30px;
    font-weight: bold;   
}
.navbar .nav>li a:hover{
    border-radius: 4px;
    background: purple!important;
    color: #fff;  
} 

.carousel-indicators {
    top: 95%;
    left: 50%;
}
.carousel{
    margin-top:-19px;
}

.carousel-control {
    top: 50%;
}
.carousel-control.right {
    right: 25px;
}

.home-content-left{
    background: #fff;
    padding:5px;
    margin-bottom:10px;
}
.home-content-left h4 {
    color: purple;
