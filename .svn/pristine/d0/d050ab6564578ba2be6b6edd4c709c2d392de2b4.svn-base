    padding:5px;
    border-bottom: 1px solid purple;
}
.home-content-left p{
    color: #000;
    padding: 9px 5px;
    border-bottom: 1px solid #eee;
   
}
.home-content-left p:hover{
    color:#fff;
    background: #DF2D32;
    border-radius: 4px;
    transition-duration: 1s;
}
.home-content-left a:hover{
    color:#fff;
}
.home-content-left img{
    margin-top:5px;
   
}
