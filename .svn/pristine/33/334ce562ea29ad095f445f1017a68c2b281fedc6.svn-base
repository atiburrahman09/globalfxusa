body{
    background: purple;
}
.dropdown-menu li a{
    padding:5px !important;
}

.iconset.top-search{
    background: #999;
}
.iconset.top-search:hover{
    background: purple;
}
span.badge.badge-important.animated.bounceIn {
    background: #E80A0A;
    color:#fff;
}

span.badge.badge-important.pull-right{
     background: #E80A0A;
     color:#fff;
}
span.badge.badge-disable.pull-right{
   background: #E80A0A;
   color:#fff;
}

span.badge.badge-important{
   background: #E80A0A;
   color:#fff; 
}

ul#main-menu-toggle-wrapper{
    margin-left: 5px;
}
.iconset.top-menu-toggle-white:hover{
    background: #fff;
}
.logo {
    margin-right: 15px;
}
.page-sidebar {
    background-color: purple !important;
}
.user-info-wrapper .user-info {
    color: #fff;
    background-color: purple;
    display: inline-block;
}
.header-seperation {
    background-color: #aaa;
}

.header .notifcation-center {
    background-color: #fff;
    border-radius: 4px;
}
.iconset {
    background: purple;
    border-radius: 4px;
}

.header .nav > li.dropdown .active {
    background-color: #fff;
    border-radius: 4px;
}

.content{
     background-color: #bbb;
}

.page-sidebar > ul > li > a {
    color: #fff;
  
}

.page-sidebar > ul > li a i {
    color: #fff;
}

.menu-title {
    color: #999;
    font-weight: bold;
}

.status-widget p {
    color: #aaa;
}

.footer-widget {   
    background-color: #eee;
}

.footer-widget .details-status {
    background-color: purple;
    color: #fff;    
}
.footer-widget a:hover, .footer-widget a:hover i {
    color: purple;  
}

.sidr {
    background-color: purple;  
}

.chat-window-wrapper input[type="text"]{
    background-color: #fff;
}

.chat-window-wrapper input[type="text"]:active{
    background-color: #999;
}

.page-content {   
    background-color: purple;    
}

.popover-content {
    width: 150px;
}

.popname{
    color:blue;
}
.page-content {
    margin-left: 0px;
    margin-top: 0px;
}
.page-title i{
    color: green;
}
.page-title i:hover{
    color: purple;
}

.label-important, .badge-important {
    background-color: purple;
}

select {
    width: 250px;
}

.home-header-top{
    padding: 15px;
}
.home-header-top ul{
    list-style: none;   
}

.home-header-top ul li{
    display: inline-block;
}
.social img{
    opacity: .3;
}
.social img:hover{
   opacity: 1; 
}
.navbar .nav>li a{   
    padding: 20px 30px;
    font-weight: bold;
   
}
.navbar .nav>li a:hover{
    border-radius: 4px;
    background: purple!important;
    color: #fff;  
} 

.carousel-indicators {
    top: 95%;
    left: 50%;
}
.carousel{
    margin-top:-19px;
}

.carousel-control {
    top: 50%;
}
.carousel-control.right {
    right: 25px;
}

.home-content-left{
    background: #fff;
    padding:5px;
    margin-bottom:10px;
}
.home-content-left h4 {
    color: purple;
    font-size: 20px;
    padding:5px;
    border-bottom: 1px solid #FF0000;
}
.home-content-left p{
    color: #000;
    padding: 9px 5px;
    border-bottom: 1px solid #eee;
   
}
.home-content-left p:hover{
    color:#fff;
    background: #DF2D32;
    border-radius: 4px;
    transition-duration: 1s;
}
.home-content-left a:hover{
    color:#fff;
}
.home-content-left img{
    margin-top:5px;
   
}
.home-content-middle{
    background: #fff;
    padding:5px;
    margin-bottom: 10px;
}
.home-content-middle h4{
    color:purple;    
}

.home-content-fight-top{
    background: #fff;
    padding:10px 5px 20px;  
    margin-bottom:10px;
}

.home-content-fight-top h4{
    color: purple;
    font-size: 20px;
    padding:5px;
    border-bottom: 1px solid #FF0000;
}

.top p:first-child{
    color:#FF0000;
    font-weight: bold;
    
}
.top p{
    line-height: 10px;
}
.two img{
    margin-top:-3px;
}
.two .btn{
    padding-top:1px;
    padding-bottom:1px;
    background: purple;
}
.two .btn:hover{
   background: red; 
}

.getway{
    background: #fff;
    padding: 10px;
}

.getway ul {
    list-style: none;    
}

.getway ul li{
    display: inline-block;
}
.button-blocks{
    padding:10px;
}
.button-blocks .dd1{
    padding: 10px;
    background: #fff;
    border-radius: 4px;
    border:1px solid #aaa;
    margin:5px;
}

.button-blocks .dd1:hover{
    border:1px solid #000;
    color: red;
}
.footer-body{
    padding:10px;
}
.footer-body ul{
    list-style: none;
}
.footer-body ul li{
    display:inline-block;
}
.footer-body a{
    color:purple;
    text-decoration: underline; 
}
.footer-body a:hover{
    color: blue;
     text-decoration: none;
   
}

.btn-abs button{
    padding: 5px 10px;
    background: purple;
    color: #fff;
    border-radius: 10px;
}
.btn-abs{
    padding-bottom: 0px;
}